frr version 8.4_git
frr defaults traditional
hostname router32
no ipv6 forwarding
!
interface lo
 ip address 10.0.0.32/32
 ip ospf area 0.0.0.3 10.0.0.32
 ip ospf passive
exit
!
interface eth0332
 ip address 173.3.32.32/26
 ip ospf area 0.0.0.3 10.0.0.32
 ip ospf network point-to-point
exit
!
interface eth3132
 ip address 10.0.0.32/32
 ip ospf area 0.0.0.3 10.0.0.32
 ip ospf network point-to-point
exit
!
interface eth3233
 ip address 10.0.0.32/32
 ip ospf area 0.0.0.3 10.0.0.32
 ip ospf network point-to-point
exit
!
router ospf
 ospf router-id 32.32.32.32
exit
!
